{
  "name": "<%APP_NAME%>",
  "version": "1.0.0",
  "description": "<%APP_DESCRIPTION%>",
  "author": "<%APP_AUTHOR%>",
  "private": true,
  "scripts": {
    "serve": "nuxt",
    "build": "nuxt build",
    "prod": "cross-env NODE_ENV=production node app.js",
    "start": "cross-env NODE_ENV=production node api/index.js",
    "generate": "cross-env NODE_ENV=production nuxt generate",
    "test": "jest",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run lint:js:fix && npm run lint:style:fix",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:style": "stylelint '**/**/*.{css,scss,vue}'",
    "lint:style:fix": "npm run lint:style -- --fix",
    "lint:prod": "NODE_ENV=production npm run lint:js && npm run lint:style",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "test:locales": "node utils/locale-diff.js",
    "deploy": "node ./deploy.js"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint:js",
    "*.{css,scss,vue}": "npm run lint:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/moment": "^1.6.0",
    "@nuxtjs/prismic": "^1.1.1",
    "cross-env": "^5.2.0",
    "maz-ui": "^0.7.4",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.9.1",
    "prismic-vue": "^2.0.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.0.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-component": "^1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "express-http-to-https": "^1.1.4",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "libphonenumber-js": "^1.7.44",
    "lint-staged": "^10.0.0",
    "mailgun-js": "^0.22.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "nuxt-material-design-icons": "^1.0.4",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.14.2",
    "v-click-outside": "^3.0.1",
    "vue-jest": "^4.0.0-0",
    "vue-virtual-scroller": "^1.0.0-rc.2",
    "vue-wait": "^1.4.8",
    "vuedraggable": "^2.23.2"
  }
}
